      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4950,
      "text": "Which, then, of your Sustainer’s powers can you disavow?",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
